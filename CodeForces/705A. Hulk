#include <bits/stdc++.h>
using namespace std;
 
//long long int jog(long long int n);
//long long int ;
 
int main()
{
    int f[1000], g[10000];
    int n,m,k,i,j;
 
    scanf("%d", &n);
 
    for(i=1; i<=n; i++)
    {
        if(i<n)
        {
            if(i%2==0)
                printf("I love that ");
            else
                printf("I hate that ");
        }
 
        else if(i==n && i%2==0)
            printf("I love it\n");
        else
            printf("I hate it\n");
    }
 
    return 0;
}
